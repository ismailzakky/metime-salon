<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity WorkingTime.
    -->
    <changeSet id="20170924133503-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="salon_id"
                                 baseTableName="working_time"
                                 constraintName="fk_working_time_salon_id"
                                 referencedColumnNames="id"
                                 referencedTableName="salon"/>

        <addForeignKeyConstraint baseColumnNames="time_period_id"
                                 baseTableName="working_time"
                                 constraintName="fk_working_time_time_period_id"
                                 referencedColumnNames="id"
                                 referencedTableName="time_period"/>

    </changeSet>
</databaseChangeLog>
